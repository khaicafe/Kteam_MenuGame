{
  "name": "vite-vue3-electron-ts-template",
  "private": true,
  "version": "0.0.1",
  "author": "Your Name",
  "main": "dist/electron/main/main.js",
  "scripts": {
    "vite:dev": "vite",
    "vite:build": "vue-tsc --noEmit && vite build",
    "vite:preview": "vite preview",
    "ts": "tsc",
    "watch": "tsc -w",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "dev": "tsc && concurrently vite \" electron .\" \"tsc -w\"",
    "app:build": "npm run vite:build && tsc && electron-builder",
    "app:preview": "npm run vite:build && tsc && electron .",
    "start": "concurrently vite \" electron .\""
  },
  "build": {
    "appId": "YourAppID",
    "asar": true,
    "directories": {
      "buildResources": "assets",
      "output": "release/${version}"
    },
    "files": [
      "dist"
    ],
    "mac": {
      "artifactName": "${productName}_${version}.${ext}",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    }
  },
  "dependencies": {
    "@tiptap/pm": "^2.0.0-beta.220",
    "@tiptap/starter-kit": "2.0.0-beta.220",
    "@tiptap/vue-3": "2.0.0-beta.220",
    "@types/aos": "^3.0.4",
    "aos": "^2.3.4",
    "apexcharts": "3.36.3",
    "axios-mock-adapter": "^1.21.2",
    "chance": "^1.1.8",
    "date-fns": "^2.29.3",
    "electron-updater": "^6.1.4",
    "axios": "1.3.4",
    "dotenv": "^16.3.1",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "electron-root-path": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^11.1.1",
    "fs.realpath": "1.0.0",
    "lodash": "^4.17.21",
    "maska": "^1.5.0",
    "pinia": "2.0.11",
    "remixicon": "2.5.0",
    "svgmap": "^2.10.1",
    "v-calendar": "3.0.0-alpha.8",
    "vee-validate": "4.9.0",
    "vite-plugin-vuetify": "1.0.2",
    "vue": "3.2.31",
    "vue-clipboard3": "^2.0.0",
    "vue-draggable-next": "^2.2.1",
    "vue-i18n": "^9.2.2",
    "vue-router": "4.0.12",
    "vue-scrollto": "^2.20.0",
    "vue-tabler-icons": "2.9.0",
    "vue3-apexcharts": "1.4.1",
    "vue3-carousel": "^0.2.9",
    "vue3-easy-data-table": "^1.4.19",
    "vue3-perfect-scrollbar": "1.6.0",
    "vue3-print-nb": "^0.1.4",
    "vue3-recaptcha-v2": "1.0.1",
    "vuedraggable": "^2.24.3",
    "vuetify": "3.3.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@mdi/font": "^6.5.95",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/chance": "^1.1.3",
    "@types/node": "^16.11.25",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "concurrently": "^8.2.1",
    "electron": "^26.2.4",
    "electron-builder": "^24.6.4",
    "esbuild": "^0.17.5",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "pinia": "^2.0.18",
    "prettier": "^2.5.1",
    "sass": "^1.49.9",
    "sass-loader": "^10.0.0",
    "typescript": "~4.5.5",
    "vite": "4.1.1",
    "vue-cli-plugin-vuetify": "2.5.5",
    "vue-router": "^4.1.3",
    "vue-tsc": "1.0.3",
    "vuetify-loader": "^2.0.0-alpha.9"
  }
}
